  <rdf:Description rdf:about="http://data.europa.eu/m8g/BusinessEvent">
    <rdfs:comment xml:lang="nl">Situatie of gebeurtenis die een specifieke behoefte of (wettelijke) vereiste vh bedrijf op een bepaald moment in zijn levenscyclus weergeeft welke vervuld kan worden door het consumeren ve bepaalde publieke dienstverlening.</rdfs:comment>
    <rdfs:label xml:lang="nl">Bedrijfsgebeurtenis</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/Channel">
    <rdfs:comment xml:lang="nl">Kanaal waarlangs een Agent kan interageren met een Publieke Dienstverlening.</rdfs:comment>
    <rdfs:label xml:lang="nl">kanaal</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/Cost">
    <rdfs:comment xml:lang="nl">Deel vd kost verbonden aan de uitvoering van de dienstverlening te betalen door de gebruiker.</rdfs:comment>
    <rdfs:label xml:lang="nl">kost</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/CriterionRequirement">
    <rdfs:comment xml:lang="nl">Criterium dat toelaat te bepalen of een gebruiker recht heeft op een dienstverlening of deze mag gebruiken.</rdfs:comment>
    <rdfs:label xml:lang="nl">Criteriumvereiste</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/Event">
    <rdfs:comment xml:lang="nl">Gebeurtenis die leidt tot, gebruik maakt van of gerelateerd is met een publieke dienstverlening.</rdfs:comment>
    <rdfs:label xml:lang="nl">Gebeurtenis</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/Evidence">
    <rdfs:comment xml:lang="nl">Elk stuk waarmee kan worden aangetoond dat men aan een CriteriumVereiste voldoet.</rdfs:comment>
    <rdfs:label xml:lang="nl">Bewijs</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/LifeEvent">
    <rdfs:comment xml:lang="nl">Belangrijke gebeurtenis of situatie in het leven van een persoon waarvoor hij/zij een publieke dienstverlening nodig heeft.</rdfs:comment>
    <rdfs:label xml:lang="nl">Levensgebeurtenis</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/Output">
    <rdfs:comment xml:lang="nl">Resultaat van een publieke dienstverlening.</rdfs:comment>
    <rdfs:label xml:lang="nl">Output</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/Participation">
    <rdfs:comment xml:lang="nl">Deelname aan een publieke dienstverlening door een agent.</rdfs:comment>
    <rdfs:label xml:lang="nl">Participatie</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/currency">
    <rdfs:comment xml:lang="nl">Munteenheid waarin de kost is gedefinieerd.</rdfs:comment>
    <rdfs:label xml:lang="nl">valuta</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/hasChannel">
    <rdfs:comment xml:lang="nl">Kanaal waarlangs een agent interageert met een publieke dienstverlening.</rdfs:comment>
    <rdfs:label xml:lang="nl">heeft kanaal</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/hasContactPoint">
    <rdfs:comment xml:lang="nl">Informatie waarmee de publieke dienstverlening kan worden gecontacteerd.</rdfs:comment>
    <rdfs:label xml:lang="nl">heeft contactpunt</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/hasCost">
    <rdfs:comment xml:lang="nl">Kostprijs verbonden aan het gebruik van de publieke dienstverlening.</rdfs:comment>
    <rdfs:label xml:lang="nl">heeft kost</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/hasCriterion">
    <rdfs:comment xml:lang="nl">Criterium waaraan de gebruiker moet voldoen om recht te hebben op een dienstverlening of om een dienstverlening te mogen gebruiken.</rdfs:comment>
    <rdfs:label xml:lang="nl">heeft criterium</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/ifAccessedThrough">
    <rdfs:comment xml:lang="nl">Associeert de kost met het kanaal waarlangs de publieke dienstverlening wordt geconsumeerd.</rdfs:comment>
    <rdfs:label xml:lang="nl">indien benaderd via</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/isGroupedBy">
    <rdfs:comment xml:lang="nl">Gebeurtenis waarop de publieke dienstverlening betrekking heeft.</rdfs:comment>
    <rdfs:label xml:lang="nl">is gegroepeerd volgens</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/processingTime">
    <rdfs:comment xml:lang="nl">Geschatte tijd nodig om de publieke dienstverlening af te leveren.</rdfs:comment>
    <rdfs:label xml:lang="nl">verwerkingstijd</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/role">
    <rdfs:comment xml:lang="nl">De rol die de participant speelt.</rdfs:comment>
    <rdfs:label xml:lang="nl">rol</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/sector">
    <rdfs:comment xml:lang="nl">Sector of bedrijvigheid waarop de publieke dienstverlening betrekking heeft of waarvoor deze is bedoeld.</rdfs:comment>
    <rdfs:label xml:lang="nl">sector</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/m8g/value">
    <rdfs:comment xml:lang="nl">Omvang van de kost, uitgedrukt in de opgegeven munteenheid.</rdfs:comment>
    <rdfs:label xml:lang="nl">waarde</rdfs:label>
  </rdf:Description>
  <owl:Ontology rdf:about="http://data.vlaanderen.be/ns/dienst">
