    <rdfs:comment xml:lang="nl">Taal waarin het aangeboden bewijs moet zijn gesteld.</rdfs:comment>
    <rdfs:label xml:lang="nl">taal</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/relation">
    <rdfs:comment xml:lang="nl">Andere publieke dienstverlening waarmee de publieke dienstverlening gerelateerd is.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Publieke dienstverlening waarmee de Gebeurtenis kan worden gekoppeld.</rdfs:comment>
    <rdfs:label xml:lang="nl">relatie</rdfs:label>
