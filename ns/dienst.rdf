  <rdf:Description rdf:about="http://purl.org/dc/terms/description">
    <rdfs:comment xml:lang="nl">Beschrijving van de gebeurtenis.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Beschrijving van de kost.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Beschrijving van de output.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Beschrijving van de participatie.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Beschrijving van de richtlijn.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Beschrijving vd publieke dienstverlening.</rdfs:comment>
    <rdfs:comment xml:lang="nl">Beschrijving vh bewijs.</rdfs:comment>
    <rdfs:label xml:lang="nl">beschrijving</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/language">
